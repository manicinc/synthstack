{
  "name": "@synthstack/api-gateway",
  "version": "0.1.0",
  "description": "SynthStack API Gateway - Fastify",
  "license": "SEE LICENSE IN ../../LICENSE",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:lite": "cp .env.lite .env && tsx watch src/index.ts",
    "dev:pro": "cp .env.pro .env && tsx watch src/index.ts",
    "build": "tsup",
    "build:lite": "cp .env.lite .env && tsup",
    "build:pro": "cp .env.pro .env && tsup",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "typecheck": "pnpm --filter @synthstack/types build && tsc --noEmit",
    "clean": "rm -rf dist node_modules",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "vitest run --config vitest.e2e.config.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@synthstack/types": "workspace:*",
    "axios": "^1.7.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.1.0",
    "@fastify/postgres": "^5.2.2",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/redis": "^6.2.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^3.1.0",
    "@octokit/rest": "^22.0.1",
    "@sentry/node": "^10.32.1",
    "@supabase/supabase-js": "^2.45.0",
    "@types/ejs": "^3.1.5",
    "@types/nodemailer": "^7.0.4",
    "bullmq": "^5.65.1",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "fastify": "^4.28.0",
    "fastify-plugin": "^4.5.1",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.3",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.11",
    "openai": "^6.10.0",
    "pdfkit": "^0.15.0",
    "pg": "^8.12.0",
    "pino-pretty": "^11.0.0",
    "resend": "^6.6.0",
    "stripe": "^17.0.0",
    "uuid": "^13.0.0",
    "yaml": "^2.8.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.13.4",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^11.0.0",
    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^2.0.0"
  },
  "optionalDependencies": {
    "@notionhq/client": "^2.2.0",
    "@slack/web-api": "^7.0.0",
    "googleapis": "^140.0.0",
    "twilio": "^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
